        <tr>
            <th>{% trans "name" %}</th>
            <th>{% trans "descr" %}</th>
            <th>{% trans "piva" %}</th>
            <th>{% trans "tel" %}</th>
            <th>{% trans "fax" %}</th>
            <th>{% trans "adr" %}</th>
            <th>{% trans "city" %}</th>
            <th>{% trans "prov" %}</th>
